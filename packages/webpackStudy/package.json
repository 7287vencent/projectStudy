{
  "name": "webpackStudy",
  "version": "1.0.0",
  "description": "webpack-vue-esline-prettier",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config ./webpack.dev.js",
    "dev2": "webpack serve --config ./webpack.dev3.js",
    "build": "webpack --config ./webpack.prod.js",
    "build2": "webpack --config ./webpack.prod2.js",
    "build:dll": "webpack --config ./webpack.dll.js",
    "dev:pro": "webpack serve --config ./webpack.prodll.js",
    "analyzer": "webpack-bundle-analyzer --port 8888 ./dist/stats.json",
    "test": "prettier --write src/**/*.{js,vue} --ignore-unknown",
    "ls-lint": "ls-lint",
    "lint:script": "eslint src/**/*.{js,vue} ",
    "lint:style": "stylelint --custom-syntax postcss-scss src/**/*.{scss,sass}",
    "lint:style-fix": "stylelint --fix --custom-syntax postcss-scss src/**/*.{scss,sass}"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.0",
    "@emmafgy/jsontoexcel": "^1.0.14",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^6.5.1",
    "dart-sass": "^1.25.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^8.3.0",
    "eslint-plugin-vue": "^8.1.1",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.5.0",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "node-notifier": "^10.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.0",
    "sass-loader": "^12.3.0",
    "speed-measure-webpack5-plugin": "^1.3.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "thread-loader": "^3.0.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.63.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@ls-lint/ls-lint": "^1.10.0",
    "element-ui": "2.15.6",
    "vue": "2.6.14",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js",
    "pre-commit": "ls-lint && lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}